<svg width="800" height="400" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#1a1a2e;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#16213e;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#0f3460;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="logo" x1="0%" y1="0%" x2="100%" y2="0%">
      <stop offset="0%" style="stop-color:#6c5ce7;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#a29bfe;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#fd79a8;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="800" height="400" fill="url(#bg)"/>
  
  <!-- Money background pattern -->
  <g opacity="0.1">
    <text x="50" y="100" font-family="Arial" font-size="60" fill="#00b894" transform="rotate(-15)">$</text>
    <text x="650" y="150" font-family="Arial" font-size="60" fill="#00b894" transform="rotate(15)">$</text>
    <text x="200" y="350" font-family="Arial" font-size="60" fill="#00b894" transform="rotate(-10)">$</text>
    <text x="550" y="350" font-family="Arial" font-size="60" fill="#00b894" transform="rotate(10)">$</text>
  </g>
  
  <!-- Main logo -->
  <g transform="translate(400,200)">
    <!-- M.J.F text -->
    <text x="-120" y="-20" font-family="Arial Black" font-size="48" font-weight="bold" fill="url(#logo)" text-anchor="middle">M.J.F</text>
    
    <!-- CARDS text -->
    <text x="-120" y="30" font-family="Arial" font-size="32" font-weight="bold" fill="white" text-anchor="middle">CARDS</text>
    
    <!-- Card icons -->
    <rect x="20" y="-40" width="60" height="35" rx="5" fill="#74b9ff" stroke="white" stroke-width="2"/>
    <rect x="30" y="-30" width="60" height="35" rx="5" fill="#0984e3" stroke="white" stroke-width="2"/>
    
    <!-- Anonymous mask silhouette -->
    <circle cx="120" cy="-10" r="25" fill="#2d3436"/>
    <path d="M105,-20 Q120,-30 135,-20 Q130,-10 135,0 Q120,10 105,0 Q110,-10 105,-20" fill="#636e72"/>
  </g>
  
  <!-- Subtitle -->
  <text x="400" y="280" font-family="Arial" font-size="18" fill="#ddd" text-anchor="middle">Premium Digital Cards & Services</text>
  
  <!-- Decorative elements -->
  <circle cx="100" cy="100" r="3" fill="#fd79a8" opacity="0.8"/>
  <circle cx="700" cy="300" r="3" fill="#6c5ce7" opacity="0.8"/>
  <circle cx="150" cy="300" r="2" fill="#a29bfe" opacity="0.6"/>
  <circle cx="650" cy="100" r="2" fill="#fd79a8" opacity="0.6"/>
</svg>